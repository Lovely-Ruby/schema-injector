#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/wangzhongqing/Desktop/Mine/前端请求插入数据库/node_modules/.pnpm/typescript@5.9.3/node_modules/typescript/bin/node_modules:/Users/wangzhongqing/Desktop/Mine/前端请求插入数据库/node_modules/.pnpm/typescript@5.9.3/node_modules/typescript/node_modules:/Users/wangzhongqing/Desktop/Mine/前端请求插入数据库/node_modules/.pnpm/typescript@5.9.3/node_modules:/Users/wangzhongqing/Desktop/Mine/前端请求插入数据库/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/wangzhongqing/Desktop/Mine/前端请求插入数据库/node_modules/.pnpm/typescript@5.9.3/node_modules/typescript/bin/node_modules:/Users/wangzhongqing/Desktop/Mine/前端请求插入数据库/node_modules/.pnpm/typescript@5.9.3/node_modules/typescript/node_modules:/Users/wangzhongqing/Desktop/Mine/前端请求插入数据库/node_modules/.pnpm/typescript@5.9.3/node_modules:/Users/wangzhongqing/Desktop/Mine/前端请求插入数据库/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/tsc" "$@"
else
  exec node  "$basedir/../../bin/tsc" "$@"
fi
